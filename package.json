  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/quantumvest/platform.git"
  },
  "keywords": [
    "quantum-computing",
    "financial-technology",
    "blockchain",
    "machine-learning",
    "error-correction",
    "variational-quantum-circuits",
    "asset-tokenization",
    "defi",
    "quantumvest"
  ],
  "author": "QuantumVest Team",
  "license": "MIT",
  "homepage": "https://quantumvest.com",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
