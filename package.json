{
  "name": "quantumvest-platform",
  "version": "1.0.0",
  "description": "Quantum Computing Platform - Re-encoding Wealth for People, Planet, Future",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:production": "NODE_ENV=production npm run build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "quantum:deploy-services": "cd services && docker-compose up -d",
    "quantum:test-algebra": "cd services/quantum-algebra && python -m pytest tests/",
    "quantum:test-scheduler": "cd services/quantum-scheduler && python -m pytest tests/",
    "quantum:test-error-correction": "cd services/quantum-fault-tolerance && python -m pytest tests/",
    "quantum:test-interface": "cd services/quantum-classical-interface && python -m pytest tests/",
    "quantum:test-ml": "cd services/quantum-ml && python -m pytest tests/",
    "infrastructure:plan": "cd infrastructure && terraform plan",
    "infrastructure:apply": "cd infrastructure && terraform apply",
    "k8s:deploy": "kubectl apply -f k8s/quantum-cloud-deployment.yaml",
    "app-runner:deploy": "cd app-runner && ./deploy.sh",
    "deploy:frontend": "vercel --prod",
    "deploy:all": "npm run infrastructure:apply && npm run k8s:deploy && npm run deploy:frontend"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-aspect-ratio": "^1.0.3",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-hover-card": "^1.0.7",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-menubar": "^1.0.4",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-progress": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slider": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
