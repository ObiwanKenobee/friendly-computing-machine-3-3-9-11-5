# QuantumVest Production Environment Configuration
# Mathematical quantum computing platform deployment

# Application Environment
NODE_ENV=production
VITE_APP_ENVIRONMENT=production
VITE_APP_VERSION=1.0.0

# API Configuration
VITE_API_BASE_URL=https://quantum-api.quantumvest.com
VITE_API_TIMEOUT=30000
VITE_API_RETRIES=3

# Quantum Service URLs
VITE_QUANTUM_ALGEBRA_URL=https://quantum-api.quantumvest.com/api/v1/algebra
VITE_QUANTUM_SCHEDULER_URL=https://quantum-api.quantumvest.com/api/v1/scheduler
VITE_QUANTUM_ERROR_CORRECTION_URL=https://quantum-api.quantumvest.com/api/v1/error-correction
VITE_QUANTUM_INTERFACE_URL=https://quantum-api.quantumvest.com/api/v1/interface
VITE_QUANTUM_ML_URL=https://quantum-api.quantumvest.com/api/v1/ml
VITE_QUANTUM_BENCHMARKS_URL=https://quantum-api.quantumvest.com/api/v1/benchmarks

# Quantum Computing Configuration
VITE_ENABLE_QUANTUM_FEATURES=true
VITE_MAX_QUBITS=20
VITE_MAX_CIRCUIT_DEPTH=100
VITE_MAX_SHOTS=100000
VITE_DEFAULT_SHOTS=1000

# Error Correction Configuration
VITE_ENABLE_ERROR_CORRECTION=true
VITE_DEFAULT_ERROR_CORRECTION_CODE=steane
VITE_ERROR_RATE_THRESHOLD=0.01
VITE_SYNDROME_MEASUREMENT_ENABLED=true

# Variational Quantum Circuit Configuration
VITE_ENABLE_VQC_TRAINING=true
VITE_MAX_VQC_QUBITS=12
VITE_MAX_VQC_LAYERS=10
VITE_DEFAULT_OPTIMIZER=adam
VITE_DEFAULT_LEARNING_RATE=0.01

# Mathematical Features
VITE_ENABLE_TENSOR_DECOMPOSITION=true
VITE_ENABLE_PARAFAC_DECOMPOSITION=true
VITE_ENABLE_TUCKER_DECOMPOSITION=true
VITE_MAX_TENSOR_RANK=16

# Scheduler Configuration
VITE_ENABLE_JOB_SCHEDULING=true
VITE_MAX_CONCURRENT_JOBS=50
VITE_JOB_TIMEOUT_SECONDS=3600
VITE_QUEUE_REFRESH_INTERVAL=5000

# Security Configuration
VITE_ENABLE_FORMAL_VERIFICATION=true
VITE_TYPE_CHECKING_ENABLED=true
VITE_BOUNDARY_VALIDATION_ENABLED=true

# Performance Configuration
VITE_ENABLE_CACHING=true
VITE_CACHE_TTL=3600
VITE_ENABLE_COMPRESSION=true
VITE_BATCH_SIZE=32

# Monitoring and Analytics
VITE_ENABLE_PERFORMANCE_MONITORING=true
VITE_ENABLE_ERROR_TRACKING=true
VITE_METRICS_COLLECTION_INTERVAL=30000

# Feature Flags
VITE_ENABLE_QUANTUM_VOLUME_BENCHMARKS=true
VITE_ENABLE_RANDOMIZED_BENCHMARKING=true
VITE_ENABLE_PROCESS_TOMOGRAPHY=false
VITE_ENABLE_EXPERIMENTAL_FEATURES=false

# WebSocket Configuration
VITE_WS_URL=wss://quantum-api.quantumvest.com/ws
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# CDN and Assets
VITE_CDN_URL=https://cdn.quantumvest.com
VITE_ASSETS_BASE_URL=/assets

# Authentication (placeholder for future implementation)
VITE_AUTH_ENABLED=false
VITE_AUTH_PROVIDER=oauth2
VITE_AUTH_DOMAIN=auth.quantumvest.com

# Regional Configuration
VITE_DEFAULT_REGION=us-east-1
VITE_SUPPORTED_REGIONS=us-east-1,us-west-2,eu-west-1,ap-southeast-1

# Compliance and Regulations
VITE_GDPR_ENABLED=true
VITE_CCPA_ENABLED=true
VITE_DATA_RETENTION_DAYS=365

# Development Features (disabled in production)
VITE_DEBUG_MODE=false
VITE_VERBOSE_LOGGING=false
VITE_ENABLE_DEVTOOLS=false
