# QuantumVest Enterprise - Production Environment Configuration
# This file shows the environment variables needed for production deployment

# Basic Application Configuration
NODE_ENV=production
VITE_APP_NAME=QuantumVest Enterprise
VITE_APP_VERSION=2.1.0
VITE_APP_ENV=production
VITE_SITE_URL=https://your-domain.vercel.app

# Build Configuration
GENERATE_SOURCEMAP=false
VITE_BUILD_TARGET=production
VITE_BUNDLE_ANALYZER=false

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PAYMENTS=true
VITE_ENABLE_ENTERPRISE_FEATURES=true
VITE_ENABLE_DEBUG_MODE=false
VITE_ENABLE_PWA=true
VITE_SECURE_MODE=true

# Performance and Optimization
VITE_INTELLIGENT_INFRASTRUCTURE=true
VITE_SNAKE_NAVIGATION=enabled
VITE_FREE_TIER_BYPASS=true
VITE_ENABLE_EDGE_OPTIMIZATION=true
VITE_PRELOAD_STRATEGY=conservative

# Regional and Localization
VITE_DEFAULT_LOCALE=en
VITE_DEFAULT_CURRENCY=USD
VITE_DEFAULT_TIMEZONE=UTC

# Third-party Services (Replace with your actual values)
# VITE_SUPABASE_URL=https://your-project.supabase.co
# VITE_SUPABASE_ANON_KEY=your_supabase_anon_key
# VITE_ANALYTICS_ID=your_analytics_id
# VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_key

# Security Headers (handled by vercel.json but can be overridden)
VITE_CSP_ENABLED=true
VITE_HSTS_ENABLED=true

# API Configuration
# VITE_API_BASE_URL=https://api.quantumvest.app
# VITE_API_TIMEOUT=30000

# Development and Testing (Disabled in production)
VITE_LOG_LEVEL=error
VITE_ENABLE_DEVTOOLS=false
VITE_ENABLE_REACT_QUERY_DEVTOOLS=false
