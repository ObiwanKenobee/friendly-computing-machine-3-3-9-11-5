# QuantumVest Enterprise Environment Variables
# Copy this file to .env.local and update with your actual values

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Maps API Configuration (for Geographical Consciousness)
REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Application Settings
VITE_APP_ENV=development
VITE_API_BASE_URL=https://api.quantumvest.app

# Analytics and Monitoring (Optional)
VITE_ANALYTICS_ID=your_analytics_id_here
VITE_SENTRY_DSN=your_sentry_dsn_here

# Feature Flags
VITE_ENABLE_DEMO_MODE=true
VITE_ENABLE_METHUSELAH_FEATURES=true
VITE_ENABLE_GEOGRAPHICAL_CONSCIOUSNESS=true

# Payment Integration (Optional)
VITE_STRIPE_PUBLISHABLE_KEY=your_stripe_key_here

# Development Settings
VITE_LOG_LEVEL=info
VITE_DEBUG_MODE=false
